{
  "name": "CondiBook",
  "description": "A bookmark management system",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "64": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs","cookies", "declarativeNetRequest"],
  "manifest_version": 3,
  "version": "0.0.1",
  "host_permissions": [
    "http://kdt-ai4-team14.elicecoding.com/",
    "http://kdt-ai4-team14.elicecoding.com/api/",
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": ["http://kdt-ai4-team14.elicecoding.com/*"]
  },

  "action": {
    "default_popup": "index.html",
    "default_title": "your project title"
  }
}
